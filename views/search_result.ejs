<%- include('./header') %>
<div class="container">

    <!-- Page Heading -->
    <div class="row">
        <div class="col-lg-12">
            <h1 class="page-header"><%= keyword %>
                <small><%= value %></small>
            </h1>
        </div>
    </div>
    <!-- /.row -->
    <% for(var i=0; i < results.length; i++) { %>
        <div class="row">
            <div class="col-md-7">
                <a href="#">
                    <img class="img-responsive" src="<%= results[i].PHOTOS %>" alt="" style="width:100%;max-width:190px;height:100%;max-height:140px;">
                </a>
            </div>
            <div class="col-md-5">
                <h3><%= results[i].TITLE %></h3>
                <h4>Star:<%= results[i].TITLE %></h4>
                <p><%= results[i].DESCRIPTION %></p>
                <a class="btn btn-primary" href="/detail?rid=<%= results[i].RID %>">Details<span class="glyphicon glyphicon-chevron-right"></span></a>
            </div>
        </div>

        <hr>
    <% } %>

    <!-- /.row -->

    <hr>

</div>
<!-- /.container -->

  <!-- jQuery -->
<script src="js/jquery.js"></script>

<!-- Bootstrap Core JavaScript -->
<script src="js/bootstrap.min.js"></script>
<%- include('./footer') %>
