<%- include('./header') %>
<div class="container">

    <!-- Page Heading -->
    <div class="row">
        <div class="col-lg-12">
            <h1 class="page-header">Meeting reports here
                <small>See how others think about this meeting</small>
            </h1>
        </div>
    </div>
    <!-- /.row -->
    <% for(var i=0; i < reports.length; i++) { %>
        <div class="row">
            <div class="col-md-7">
                <a href="#">
                    <img class="img-responsive" src="<%= reports[i].PHOTOS %>" alt="" style="width:100%;max-width:190px;height:100%;max-height:140px;">
                </a>
            </div>
            <div class="col-md-5">
                <h3><%= reports[i].nick_name %></h3>
                <p><%= reports[i].description %></p>
            </div>
        </div>

        <hr>
    <% } %>


    <!-- Footer -->
    <footer>
        <div class="row">
            <div class="col-lg-12">
                <p>Copyright &copy; Your Website 2014</p>
            </div>
        </div>
        <!-- /.row -->
    </footer>

</div>
<!-- /.container -->

  <!-- jQuery -->
<script src="js/jquery.js"></script>

<!-- Bootstrap Core JavaScript -->
<script src="js/bootstrap.min.js"></script>
<%- include('./footer') %>
